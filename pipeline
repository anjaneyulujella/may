pipeline
{
agent any
stages
{
stage ('pull code from scm')
{
steps 
{
git credentialsId: 'mycred', url: 'https://github.com/anjaneyulujella/jella.git'
}
}
stage ('build docker file')
{
steps
{
sh 'docker build -t uma .'
}
}
stage ('delete container if already exists')
{
steps
{
sh 'docker rm -f anji'
}
}
stage ('create container')
{
steps
{
sh 'docker run -dti --name anji -p 91:80 uma /bin/bash'
}
}
}
}
